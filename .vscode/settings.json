{
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "never"
    },
    "search.exclude": {
        "**/.yarn": true,
        "**/.pnp.*": true
    },
    "typescript.tsdk": ".yarn/sdks/typescript/lib",
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "svg.preview.background": "editor",
    "files.eol": "\n",
    "eslint.nodePath": ".yarn/sdks",
    "eslint.experimental.useFlatConfig": true,
    "prettier.prettierPath": ".yarn/sdks/prettier/index.cjs",
    "testing.openTesting": "neverOpen",
    "jest.outputConfig": {
        "revealOn": "run",
        "revealWithFocus": "none",
        "clearOnRun": "none"
    },
    "jest.runMode": {
        "type": "on-demand"
    },
    "jest.jestCommandLine": "yarn test",
    "playwright.env": {
        "NODE_OPTIONS": "-r ./.pnp.cjs"
    },
    "vs-code-prettier-eslint.prettierLast": false,
    "eslint.run": "onType",
    "yaml.schemas": {
        "file:///home/dsigulski/.vscode/extensions/atlassian.atlascode-3.0.10/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml",
        "https://json.schemastore.org/github-workflow.json": "file:///d%3A/projects/tune-tools/.github/workflows/deploy.yml"
    }
}
