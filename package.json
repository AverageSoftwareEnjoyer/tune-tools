{
  "name": "tune-tools",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage || exit 0",
    "test:watch": "jest --watch",
    "e2e": "playwright test",
    "e2e:report": "playwright show-report",
    "e2e:ui": "playwright test --ui",
    "lint": "eslint \"src/**/*.{js,ts,html}\" --fix",
    "format": "prettier \"src/**/*.{js,ts,html,css,scss}\" --write",
    "postinstall": "husky install",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org tune-tools --project tune-tools ./dist && sentry-cli sourcemaps upload --org tune-tools --project tune-tools ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.1",
    "@angular/cdk": "^18.0.1",
    "@angular/common": "^18.0.1",
    "@angular/compiler": "^18.0.1",
    "@angular/core": "^18.0.1",
    "@angular/forms": "^18.0.1",
    "@angular/material": "^18.0.1",
    "@angular/platform-browser": "^18.0.1",
    "@angular/platform-browser-dynamic": "^18.0.1",
    "@angular/platform-server": "^18.0.1",
    "@angular/router": "^18.0.1",
    "@sentry/angular": "^8.7.0",
    "@sentry/cli": "^2.32.1",
    "@supabase/supabase-js": "^2.43.4",
    "drizzle-orm": "^0.31.0",
    "postgres": "^3.4.4",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1800.2",
    "@angular-devkit/build-angular": "^18.0.2",
    "@angular-devkit/core": "^18.0.2",
    "@angular-devkit/schematics": "^18.0.2",
    "@angular-eslint/builder": "^18.0.1",
    "@angular-eslint/eslint-plugin": "^18.0.1",
    "@angular-eslint/eslint-plugin-template": "^18.0.1",
    "@angular-eslint/schematics": "^18.0.1",
    "@angular-eslint/template-parser": "^18.0.1",
    "@angular/cli": "^18.0.2",
    "@angular/compiler-cli": "^18.0.1",
    "@eslint/js": "^9.4.0",
    "@playwright/test": "^1.44.1",
    "@sentry/wizard": "^3.22.3",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/eslint": "^8.56.10",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.13.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@typescript-eslint/types": "^7.11.0",
    "@typescript-eslint/utils": "^7.11.0",
    "angular-cli-ghpages": "2.0.0",
    "ast-types": "^0.16.1",
    "drizzle-kit": "^0.22.1",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jsdoc": "^48.2.7",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.4",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-prettier": "^5.1.3",
