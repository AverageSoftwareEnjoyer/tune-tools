    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.1.0",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.0",
    "recast": "^0.23.9",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0"
  },
  "packageManager": "yarn@4.1.1",
  "lint-staged": {
    "src/**/*.{js,ts,html,scss}": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  }
}
