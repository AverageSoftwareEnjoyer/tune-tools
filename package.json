{
    "name": "tune-tools",
    "version": "0.0.1",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch",
        "e2e": "playwright test",
        "e2e:report": "playwright show-report",
        "e2e:ui": "playwright test --ui",
        "lint": "npx eslint \"src/**/*.{js,ts}\" --fix",
        "format": "npx prettier \"src/**/*.{js,ts,html,css,scss}\" --write",
        "postinstall": "husky install",
        "sentry:sourcemaps": "sentry-cli sourcemaps inject --org tune-tools --project tune-tools ./dist && sentry-cli sourcemaps upload --org tune-tools --project tune-tools ./dist"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.3.0-rc",
        "@angular/cdk": "^17.3.0-rc",
        "@angular/common": "^17.3.0-rc",
        "@angular/compiler": "^17.3.0-rc",
        "@angular/core": "^17.3.0-rc",
        "@angular/forms": "^17.3.0-rc",
        "@angular/material": "17.3.0-rc.0",
        "@angular/platform-browser": "^17.3.0-rc",
        "@angular/platform-browser-dynamic": "^17.3.0-rc",
        "@angular/platform-server": "^17.3.0-rc",
        "@angular/router": "^17.3.0-rc",
        "@sentry/angular-ivy": "^7.106.0",
        "@sentry/cli": "^2.30.0",
        "@supabase/supabase-js": "^2.39.7",
        "drizzle-orm": "^0.30.1",
        "postgres": "^3.4.3",
        "rxjs": "~7.8.1",
        "ts-node": "^10.9.2",
        "tslib": "^2.6.2",
        "zone.js": "~0.14.4"
    },
    "devDependencies": {
        "@angular-devkit/architect": "^0.1703.0-rc.0",
        "@angular-devkit/build-angular": "^17.3.0-rc",
        "@angular-devkit/core": "^17.3.0-rc",
        "@angular-eslint/builder": "^17.2.1",
        "@angular-eslint/eslint-plugin": "^17.2.1",
        "@angular-eslint/eslint-plugin-template": "^17.2.1",
        "@angular-eslint/schematics": "^17.2.1",
        "@angular-eslint/template-parser": "^17.2.1",
        "@angular/cli": "^17.3.0-rc",
        "@angular/compiler-cli": "^17.3.0-rc",
        "@playwright/test": "^1.42.1",
        "@types/eslint": "^8.56.5",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.25",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@typescript-eslint/types": "^7.1.1",
        "angular-cli-ghpages": "2.0.0-beta.2",
        "drizzle-kit": "^0.20.14",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard-with-typescript": "^43.0.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsdoc": "^48.2.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-no-relative-import-paths": "^1.5.3",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-simple-import-sort": "^12.0.0",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-preset-angular": "^14.0.3",
        "jsonc-eslint-parser": "^2.4.0",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "typescript": "^5.4.2"
    },
    "packageManager": "yarn@4.1.1",
    "lint-staged": {
        "src/**/*.{js,ts,html,scss}": [
            "eslint --quiet --fix",
            "prettier --write --ignore-unknown"
        ]
    }
}
