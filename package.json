{
  "name": "tune-tools",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage || exit 0",
    "test:watch": "jest --watch",
    "e2e": "playwright test",
    "e2e:report": "playwright show-report",
    "e2e:ui": "playwright test --ui",
    "lint": "eslint \"src/**/*.{js,ts,html}\" --fix",
    "format": "prettier \"src/**/*.{js,ts,html,css,scss}\" --write",
    "postinstall": "husky install",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org tune-tools --project tune-tools ./dist && sentry-cli sourcemaps upload --org tune-tools --project tune-tools ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.2",
    "@angular/cdk": "^18.2.2",
    "@angular/common": "^18.2.2",
    "@angular/compiler": "^18.2.2",
    "@angular/core": "^18.2.2",
    "@angular/forms": "^18.2.2",
    "@angular/material": "^18.2.2",
    "@angular/platform-browser": "^18.2.2",
    "@angular/platform-browser-dynamic": "^18.2.2",
    "@angular/platform-server": "^18.2.2",
    "@angular/router": "^18.2.2",
    "@sentry/angular": "^8.28.0",
    "@sentry/cli": "^2.34.1",
    "@supabase/supabase-js": "^2.45.3",
    "drizzle-orm": "^0.32.2",
    "postgres": "^3.4.4",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1802.2",
    "@angular-devkit/build-angular": "^18.2.2",
    "@angular-devkit/core": "^18.2.2",
    "@angular-devkit/schematics": "^18.2.2",
    "@angular-eslint/builder": "^18.3.0",
    "@angular-eslint/eslint-plugin": "^18.3.0",
    "@angular-eslint/eslint-plugin-template": "^18.3.0",
    "@angular-eslint/schematics": "^18.3.0",
    "@angular-eslint/template-parser": "^18.3.0",
    "@angular/cli": "^18.2.2",
    "@angular/compiler-cli": "^18.2.2",
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.46.1",
    "@sentry/wizard": "^3.28.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.3",
    "@types/ungap__structured-clone": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@typescript-eslint/types": "^8.4.0",
    "@typescript-eslint/utils": "^8.4.0",
    "@ungap/structured-clone": "^1.2.0",
    "angular-cli-ghpages": "2.0.1",
    "ast-types": "0.16.1",
    "drizzle-kit": "^0.23.2",
    "eslint": "^9.9.1",
    "eslint-plugin-jest": "^28.8.2",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
